include ../section-header/section-header.pug
include ../experience-list/experience-list.pug

mixin education(content)
    - const { items } = content;
    - const title = 'education';
    - const imagePath = 'assets/img/education-icon.svg';
    section.education
        .education__section-header
            +sectionHeader({ title, imagePath, isDistant: true })
        .education__list
            +experienceList(items, {titleColor: "blue"})